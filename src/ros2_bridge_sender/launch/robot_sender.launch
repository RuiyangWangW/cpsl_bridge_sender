from launch import LaunchDescription
from launch_ros.actions import Node

def generate_launch_description():
    return LaunchDescription([
        Node(
            package='ros2_sender_pkg',
            executable='scan_sender',
            name='scan_sender',
            output='screen'
        ),
        Node(
            package='ros2_sender_pkg',
            executable='tf_sender',
            name='tf_sender',
            output='screen'
        )
    ])
